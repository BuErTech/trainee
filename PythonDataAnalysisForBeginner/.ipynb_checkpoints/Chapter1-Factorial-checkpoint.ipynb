    "def factorial_normal(n):\n",
    "    result = 1\n",
    "    for i in range(n):\n",
    "        result = result*n\n",
    "        n = n-1\n",
    "    return result\n",
    "\n",
    "factorial_normal(5)"
   ]
  },
