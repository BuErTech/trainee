   "source": [
    "import time\n",
    "\n",
    "\n",
    "def printtime(func):\n",
    "    def wrapper(*args, **kwargs):\n",
    "        print(time.ctime())\n",
    "        return func(*args, **kwargs)\n",
    "\n",
    "    return wrapper\n",
    "\n",
    "\n",
    "@printtime\n",
    "def printhello(name):\n",
    "    print('Hello', name)\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    printhello('Sam')"
   ]
