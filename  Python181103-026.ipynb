    "#!/usr/bin/python\n",
    "# -*- coding: UTF-8 -*-\n",
    "def fact(j):\n",
    "    sum = 0\n",
    "    if j == 0:\n",
    "        sum = 1\n",
    "    else:\n",
    "        sum = j * fact(j - 1)\n",
    "    return sum\n",
    "\n",
    "\n",
    "print(fact(5))"
